{
    "extends": "airbnb",
    "rules":{
        "linebreak-style": 0,
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "jsx-a11y/anchor-is-valid": [ "error", {
            "components": [ "Link" ],
            "specialLink": [ "hrefLeft", "hrefRight", "to" ],
            "aspects": [ "noHref", "invalidHref", "preferButton" ]
          }]
    },
    "settings": {
        "import/core-modules": [
            "react",
            "prop-types",
            "react-helmet",
            "styled-components"
          ]
    }
}